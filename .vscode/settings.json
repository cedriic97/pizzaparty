{
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 300000,
    "files.exclude": {
      "**/.git": true,
      "**/.DS_Store": true,
      "**/*.spec.ts": false,
      "**/*.spec.json": true,
      "**/*.app.json": false,
      "**/polyfills.ts": true,
      "**/main.ts": true,
      "**/*.conf.js": true,
      "**/browserlist": true,
      "**/test.ts": true,
    },
    "files.hotExit": "onExit",
    "files.defaultLanguage": "typescript",
    "files.trimTrailingWhitespace": true,
    "cSpell.words": [
      "DROPDOWN",
      "ONEIDEAFORM",
      "Queryable",
      "imgpath"
    ],
    "cSpell.enabledLanguageIds": [
      "asciidoc",
      "c",
      "cpp",
      "csharp",
      "css",
      "go",
      "handlebars",
      "html",
      "jade",
      "javascript",
      "javascriptreact",
      "json",
      "latex",
      "less",
      "markdown",
      "php",
      "plaintext",
      "pub",
      "python",
      "restructuredtext",
      "rust",
      "text",
      "typescript",
      "typescriptreact",
      "yml"
    ],
}